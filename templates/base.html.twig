            <div class="progress-bar"></div ><div class="status typo">0%</div>
        </div>
    </div>
</div>

{% embed oMe.type ~ "/menu.html.twig" %}{% endembed %}
{% block header %}{% endblock %}
{% block body %}{% endblock %}
{% block footer %}{% endblock %}
<footer class="footer">
    <div class="container">
      <p class="m-b m-t">
        <a href="/home/mentions-legales.html">Mentions légales</a>  |  <a href="https://github.com/mavoix/candidature">Code source</a>
      </p>
    </div>
</footer>
<div class="modal fade" tabindex="-1" role="dialog" id="modalConfirm">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" >
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h2 class="modal-title text-primary" id="modal-title" ></h2>
            </div>
            <div class="modal-body" id="modal-body">

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default typo" data-dismiss="modal" id="modal-cancel">Annuler</button>
                <button type="button" class="btn btn-primary typo" id="modal-confirm">Ok</button>
            </div>
        </div>
    </div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
